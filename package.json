{
  "author": "Pedro Martin <pedro.martin@red-badger.com>",
  "description": "a script to import airtable stories to gh issues",
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=13.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "yarn prettier --write"
    ],
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "main": "src/index.ts",
  "name": "airtable-to-gh-issues",
  "private": false,
  "repository": "git@github.com:redbadger/airtable-to-gh-issues.git",
  "scripts": {
    "lint": "yarn eslint",
    "start": "yarn ts-node ./src/index.ts"
  },
  "version": "1.0.0",
  "dependencies": {
    "@octokit/rest": "^16.43.1",
    "csv-parse": "^4.8.5",
    "dotenv": "^8.2.0"
  }
}
